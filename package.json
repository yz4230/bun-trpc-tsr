{
  "name": "bun-trpc-tsr",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "conc bun:dev:client bun:dev:server",
    "dev:client": "vite --port 3000 --clearScreen=false",
    "dev:server": "bun --watch run --no-clear-screen src/server.ts",
    "build": "NODE_ENV=production conc bun:build:client bun:build:server",
    "build:client": "vite build",
    "build:server": "bun run build-server.ts",
    "serve": "bun dist/server.js",
    "test": "vitest run",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@tanstack/react-query": "^5.74.4",
    "@tanstack/react-query-devtools": "^5.74.6",
    "@tanstack/react-router": "^1.117.0",
    "@tanstack/react-router-devtools": "^1.117.0",
    "@tanstack/router-plugin": "^1.117.0",
    "@trpc/client": "^11.1.1",
    "@trpc/server": "^11.1.1",
    "@trpc/tanstack-react-query": "^11.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "es-toolkit": "^1.35.0",
    "lucide-react": "^0.488.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "latest",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "lefthook": "^1.11.11",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.3",
    "vitest": "^3.1.2",
    "web-vitals": "^4.2.4"
  }
